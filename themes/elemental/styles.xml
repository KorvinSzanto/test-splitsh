<?xml version="1.0"?>
<styles version="1.0">
    <webfonts>
        <skin identifier="night-road">
            <font type="google" name="Abril Fatface" />
            <font type="google" name="Oxygen" />
            <font type="google" name="Oswald" />
        </skin>
        <skin identifier="default">
            <font type="google" name="Titillium Web" />
        </skin>
    </webfonts>
    <set name="Colors">
        <style name="Primary Color" variable="primary" type="color"/>
        <style name="Secondary Color" variable="secondary" type="color"/>
    </set>
    <set name="Header">
        <style
                name="Logo"
                variable="logo"
                type="type"
        />
    </set>
    <set name="Image Slider">
        <style
                name="Background Image"
                variable="image-slider-background-image"
                type="image"
        />
    </set>
</styles>